            ]
        },
        {
            "type": "TABLE",
            "name": "Details Table",
            "elements": [
                "Predators",
                "Prey",
                "Predator Deaths",
                "Predator Births",
                "Prey Births",
                "Prey Deaths"
            ]
        }
    ]
